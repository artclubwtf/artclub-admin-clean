{
  "name": "artclub-platform",
  "private": true,
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev:admin": "pnpm --filter admin dev",
    "build:admin": "pnpm --filter admin build",
    "dev:mobile": "pnpm --filter mobile dev",
    "ios:dev": "pnpm --filter mobile ios",
    "ios:build": "cd apps/mobile && eas build -p ios --profile preview",
    "ios:submit": "cd apps/mobile && eas submit -p ios --latest",
    "lint": "turbo lint",
    "build": "turbo build",
    "test": "turbo test"
  },
  "devDependencies": {
    "turbo": "^2.0.0"
  }
}
